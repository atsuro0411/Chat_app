    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build:css": "sass --load-path app/assets/stylesheets app/assets/stylesheets/application.scss app/assets/builds/application.css --style=compressed --source-map --source-map-urls=relative",
    "build:css:pretty": "sass --load-path app/assets/stylesheets app/assets/stylesheets/application.scss app/assets/builds/application.css --style=expanded --source-map --source-map-urls=relative",
    "watch:css": "sass --watch --load-path app/assets/stylesheets app/assets/stylesheets/application.scss:app/assets/builds/application.css --style=expanded --source-map --source-map-urls=relative"
  },
